[% BLOCK button %]
<a href="[% request.url_for(url) %]" class="toggle button [% IF real %]client_real[% ELSE %]client_logged_out[% END %] invisible">
    <span[% IF class %] class="[% class %]"[% END %]>[% text %]</span>
</a>
[% END %]

<div id='cashier-content'>
    <h1>[% l('Available payment methods') %]</h1>
    <div class="gr-12">
        <div id="payment_method_suggestions gr-padding-10" class='center-text'>
            <div class="gr-padding-10 invisible upgrademessage">
                <a class="button"></a>
            </div>
            <p>
                [% INCLUDE button url='/' text=l('Open an account now') %]
                [% INCLUDE button url='cashier/forwardws#deposit' real=1 class='deposit' text=l('Deposit') %]
                [% INCLUDE button url='cashier/forwardws#withdraw' real=1 class='withdraw' text=l('Withdraw') %]
            </p>
        </div>
    </div>

    [% BLOCK row %]
        <div class="gr-row gr-padding-10 [% is_header ? 'table-header' : 'table-body' %]">
            <div class="gr-2 gr-6-m">[% IF id %]<div id="[% id %]" class="payment-logo"></div>[% ELSE %][% method %][% END %]</div>
            <div class="gr-2 gr-6-m">[% currencies %]</div>
            <div class="gr-2 gr-hide-m">[% deposit.join('<br />') %]</div>
            <div class="gr-3 gr-hide-m">[% withdrawal.join('<br />') %]</div>
            <div class="gr-3 gr-hide-m">[% time.join('<br />') %][% is_header ? '*' : '' %]</div>
        </div>
    [% END %]
    [% BLOCK section %]
        <div class="gr-row gr-padding-10">
            <div class="gr-12">
                <h3>[% title %]</h3>
            </div>
        </div>
        [% INCLUDE row is_header=1 method=l('Method') currencies=l('Currencies') deposit=l('Min-Max Deposit') withdrawal=withdrawal || l('Min-Max Withdrawal') time=l('Processing Time') %]
    [% END %]


    <div id="payment_methods">
    [% INCLUDE section title=l('Bank wire/Money transfer') %]
    [% INCLUDE row id='int_bank_wire'       currencies='USD GBP EUR AUD' deposit='500 - 100,000'  withdrawal='500 - 100,000' time=[l('Deposit: [_1] working day', 1), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='local_bank_transfer' currencies='USD GBP EUR AUD' deposit='25 - 10,000'    withdrawal='25 - 10,000' time=[l('Deposit: [_1] working day', 1), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='western_union'       currencies='USD GBP EUR AUD' deposit='50 - 250'       withdrawal='50 - 250'    time=[l('Deposit: [_1] working day', 1), l('Withdrawal: [_1] working day', 1)] %]

    [% INCLUDE section title=l('Credit/Debit Card') %]
    [% INCLUDE row id='visa'            currencies='USD GBP EUR AUD' deposit='10 - 10,000' withdrawal='10 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='mastercard'      currencies='USD EUR AUD'     deposit='10 - 10,000' withdrawal='10 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='cuo_credit_card' currencies='USD'             deposit='10 - 1,000'  withdrawal='N/A'         time=[l('Deposit: Instant'), l('Withdrawal: Not applicable')] %]

    [% INCLUDE section title=l('E-cash') %]
    [% INCLUDE row id='okpay'         currencies='USD EUR'         deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='fastpay'       currencies='USD'             deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='perfect_money' currencies='USD EUR'         deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='moneybrokers'  currencies='USD GBP EUR AUD' deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='neteller'      currencies='USD GBP EUR AUD' deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='moneta'        currencies='USD GBP EUR AUD' deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='webmoney'      currencies='USD EUR'         deposit='5 - 10,000'  withdrawal='5 - 10,000' time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='qiwi'          currencies='USD EUR'         deposit=['5 - 200 (USD)', '5 - 150 (EUR)'] withdrawal=['5 - 200 (USD)', '5 - 150 (EUR)'] time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='yandex'        currencies='USD'             deposit='25 - 10,000' withdrawal='N/A'        time=[l('Deposit: Instant'), l('Withdrawal: Not Applicable')] %]
    [% INCLUDE row id='paysafecard'   currencies='USD GBP EUR AUD' deposit='5 - 1,000'   withdrawal='5 - 750'    time=[l('Deposit: Instant'), l('Withdrawal: [_1] working day', 1)] %]

    [% INCLUDE section title=l('Cryptocurrencies') withdrawal=l('Min Withdrawal') %]
    [% INCLUDE row id='btc' currencies='BTC' deposit='0.002' withdrawal='0.002' time=[l('Deposit: 3 blockchain confirmations'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='bch' currencies='BCH' deposit='0.01'  withdrawal='0.01'  time=[l('Deposit: 3 blockchain confirmations'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='eth' currencies='ETH' deposit='0.01'  withdrawal='0.01'  time=[l('Deposit: 3 blockchain confirmations'), l('Withdrawal: [_1] working day', 1)] %]
    [%# INCLUDE row id='etc' currencies='ETC' deposit='0.002' withdrawal='0.002' time=[l('Deposit: 3 blockchain confirmations'), l('Withdrawal: [_1] working day', 1)] %]
    [% INCLUDE row id='ltc' currencies='LTC' deposit='0.1'   withdrawal='0.1'   time=[l('Deposit: 3 blockchain confirmations'), l('Withdrawal: [_1] working day', 1)] %]
    </div>

    <div class="gr-padding-10">
        <p class="hint">*[% l('All your deposits and withdrawals are processed by [_1] within 24 hours. However, there may be additional processing time required by your bank or money transfer service.', website_name) %]</p>
    </div>
</div>
