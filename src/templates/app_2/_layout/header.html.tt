    <nav id="icon-with-text-demo" class="mdc-temporary-drawer__content mdc-list">
        <a class="mdc-list-item mdc-temporary-drawer--selected" href="#">Alert</a>
        <a class="mdc-list-item" href="#">Alert</a>
